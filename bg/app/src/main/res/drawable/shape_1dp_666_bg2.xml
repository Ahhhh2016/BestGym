<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape>
            <solid android:color="@color/transtr" />
        </shape>
    </item>

    <item
        android:bottom="-1dp"
        android:left="-1dp"
        android:right="1dp"
        android:top="-1dp">
        <shape>
            <stroke
                android:width="1px"
                android:color="@color/text_d2" />
        </shape>
    </item>
</layer-list>