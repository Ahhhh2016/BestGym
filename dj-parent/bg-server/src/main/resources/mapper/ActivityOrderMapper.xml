<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.dao.ActivityOrderMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.demo.domain.ActivityOrder">
        <id column="id" property="id" />
        <result column="facility_id" property="facilityId" />
        <result column="activityId" property="activityid" />
        <result column="userId" property="userid" />
        <result column="starttime" property="starttime" />
        <result column="endtime" property="endtime" />
        <result column="period" property="period" />
        <result column="paymentStatus" property="paymentstatus" />
        <result column="usageStatus" property="usagestatus" />
        <result column="QRcode" property="qrcode" />
        <result column="paymentMoney" property="paymentmoney" />
        <result column="paymentDate" property="paymentdate" />
        <result column="username" property="username" />
        <result column="useremail" property="useremail" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, facility_id, activityId, userId, starttime, endtime, period, paymentStatus, usageStatus, QRcode, paymentMoney, paymentDate, username, useremail
    </sql>

</mapper>
